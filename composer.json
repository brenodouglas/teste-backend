{
    "name": "countries/php",
    "description": "Aplicação para teste backend do grupo Citar",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Breno Douglas",
            "email": "bdouglasans@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "slim/slim": "^3.0",
        "slim/twig-view": "dev-master",
        "phpoffice/phpspreadsheet": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
